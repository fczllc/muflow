{
  "name": "muflow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "author": "tianyi",
  "description": "A Vue Flow based flow chart editor with enhanced features",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/muflow"
  },
  "keywords": [
    "vue",
    "flow",
    "flowchart",
    "diagram",
    "vue-flow"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:unit": "vitest --environment jsdom --root src/"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@types/uuid": "^10.0.0",
    "@vue-flow/core": "^1.42.1",
    "feather-icons": "^4.29.2",
    "html2canvas": "^1.4.1",
    "lodash-es": "^4.17.21",
    "uuid": "^11.1.0",
    "vue": "^3.4.19"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.5",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "html-to-image": "^1.11.13",
    "jsdom": "^26.0.0",
    "typescript": "~5.7.2",
    "vite": "^6.1.0",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.0"
  }
}
